<div>
	<div ng-repeat="q in questions">
		<p>{{q.q}}</p>
		<label class="checkbox inline">
		<input type="radio"  name="{{q.q}}" id="{{q.q}}1" value="1">{{q.options[1]}}
		<input type="radio"  name="{{q.q}}" id="{{q.q}}2" value="2">{{q.options[2]}}
		<input type="radio"  name="{{q.q}}" id="{{q.q}}3" value="3">{{q.options[3]}}
		<input type="radio"  name="{{q.q}}" id="{{q.q}}4" value="4">{{q.options[4]}}
			
		</label>	
	</div>
	<hr/>
	<button class="btn btn-success" type="submit">Submit</button>
</div>