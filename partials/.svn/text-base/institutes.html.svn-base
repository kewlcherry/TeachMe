<ul class="thumbnails">
	<li ng-repeat="institute in institutes" >
		<a href="#/institute/{{institute.id}}" class="thumbnail plain" style="width: 150px;text-decoration: none;position:relative;">
			<div style="position:relative;height:150px;width: 150px;">
				<img ng-src="{{institute.imgUrl}}"/>
				<div class="input-block-level" style="background-color: black;opacity: .5;color: #ffffff;padding: 3px; position:absolute;bottom:0px;">{{institute.name}}</div>
			</div>
		</a>
	</li>

</ul>